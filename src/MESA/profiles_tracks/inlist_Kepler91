
! inlist_project

! contains the parameters controlling this job

! for the sake of future readers of this (yourself included),
! ONLY include the controls you are actually using.
! DO NOT include all of the other controls that
! simply have their default values.



&star_job

      mesa_dir = '' ! empty string defaults to environment variable MESA_DIR

      create_pre_main_sequence_model = .true.

      change_net = .true. ! switch nuclear reaction network
      new_net_name = 'o18_and_ne22.net'

      eos_file_prefix = 'mesa'
      kappa_file_prefix = 'gs98'

      history_columns_file = 'history_columns.list'

      pgstar_flag = .false.

/ !end of star_job

&controls

   !max_model_number = 15

  !From Lillo-Box+ (2013) -- 
  !  http://arxiv.org/pdf/1312.3943v1.pdf -- The analyses in this paper 
  !  involve several unusual techniques and assumptions, so there's a 
  !  good chance these parameters will be revised.
  !
  ! starting specifications
      initial_mass = 1.31 
  !Converting from the [Fe/H] = 0.11 value given in Lillo-Box, p. 6.
  !  I then used the fe2h.pro to convert to Z. This IDL routine is
  !  in this directory.
      initial_Z = 0.025743645
  !This is the solar value -- Vorontsov+ (1991).
      initial_Y = 0.25

   ! output to files and terminal
      photostep = 50
      profile_interval = 50
      history_interval = 1
      terminal_interval = 10
      write_header_frequency = 10

  ! mass gain or loss
      RGB_wind_scheme = 'Reimers'
      AGB_wind_scheme = 'Blocker'
      RGB_to_AGB_wind_switch = 1d-4
      Reimers_wind_eta = 0.8d0
      Blocker_wind_eta = 0.5d0

  ! when to stop
      max_age = 10d9

/

&pgstar

      ! top level controls

         !pause = .true.
            ! if true, the code waits for user to enter a RETURN on the command line



/ ! end of pgstar namelist
